{
  "name": "words-server",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development ts-node index.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "generate": "ts-node codegen.ts",
    "test": "npm run db:seed:test && jest --watch",
    "db:seed:dev": "bash db/seed/dev-seed.sh",
    "db:seed:test": "bash db/seed/test-seed.sh"
  },
  "packageManager": "yarn@3.3.0",
  "engines": {
    "node": "18.12.1"
  },
  "type": "module",
  "dependencies": {
    "@apollo/server": "^4.2.1",
    "@graphql-tools/merge": "^8.2.3",
    "apollo-server-lambda": "^3.6.3",
    "custom-env": "^2.0.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.14.1",
    "pg": "^8.7.3",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/core": "^2.5.1",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-resolvers": "2.5.0",
    "@graphql-tools/load-files": "^6.5.3",
    "@types/jest": "^29.2.3",
    "@types/pg": "^8.6.4",
    "@types/pg-format": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "prettier": "2.8.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
