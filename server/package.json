{
  "name": "words-server",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development ts-node index.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "npm run db:seed:test && jest --watch",
    "db:seed:dev": "bash db/seed/dev-seed.sh",
    "db:seed:test": "bash db/seed/test-seed.sh",
    "generate": "graphql-codegen-esm --config codegen.yml"
  },
  "packageManager": "yarn@3.3.1",
  "engines": {
    "node": "18.12.1"
  },
  "type": "module",
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.14",
    "custom-env": "^2.0.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "pg-format": "^1.0.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/typescript": "2.8.5",
    "@graphql-codegen/typescript-resolvers": "2.7.10",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.11.17",
    "@types/pg": "^8.6.6",
    "@types/pg-format": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
